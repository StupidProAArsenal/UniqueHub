local function YKTCXTK_fake_script() -- text1.LocalScript 
	local script = Instance.new('LocalScript', text1)

	local getgamename = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name -- you may use this for your game hub... just for easy game-name grab instend of typing it everytime (will auto-update eventally)
	local workinglol = true
	local ConfirmWorking = "NO TEXT"
	if workinglol==true then ConfirmWorking="Yes" else ConfirmWorking="No" end
	script.Parent.Text = "Game: " ..getgamename.. " | U-Hub v2 Working: " ..ConfirmWorking
end
coroutine.wrap(YKTCXTK_fake_script)()
local function GPYRJLN_fake_script() -- Main.MainScript 
	local script = Instance.new('LocalScript', Main)

	local v = script.Parent.Parent.Main
	
	
	local f = false
	local t = true
	
	v.Size = UDim2.new(0, 0, 0, 0)
	
	
	for i,things in pairs(v:GetChildren()) do
	
		if things:IsA("Frame") then
			things.Visible = f
		end
		if things:IsA("TextLabel") then
			things.Visible = f
		end
		if things:IsA("ScrollingFrame") then
			things.Visible = f
		end
	
	
	end
	
	if not game:IsLoaded() then
		game:IsLoaded():Wait()
	end
	
	wait(1.5)
	
	v:TweenSize(UDim2.new(0, 313,0, 307), "InOut", "Quint", 1)
	
	task.wait(1)
	
	for i,things in pairs(v:GetChildren()) do
	
		if things:IsA("Frame") then
			things.Visible = t
		end
		if things:IsA("TextLabel") then
			things.Visible = t
		end
		if things:IsA("ScrollingFrame") then
			things.Visible = t
		end
	
	
	end
	
	script.Parent.notify.Visible = false
end
coroutine.wrap(GPYRJLN_fake_script)()
