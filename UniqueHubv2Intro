local function ZGAYBKJ_fake_script() --[[ LoadScript.mousenter ]]
	local script = Instance.new('LocalScript', LoadScript)

	script.Parent.MouseEnter:Connect(function()
		LoadScript.BackgroundColor3 = Color3.new(0.415686, 0.545098, 0.807843)
	end)
	
	script.Parent.MouseLeave:Connect(function()
		LoadScript.BackgroundColor3 = Color3.new(0.372549, 0.486275, 0.72549)
	end)
end
coroutine.wrap(ZGAYBKJ_fake_script)()
local function UNCWKBO_fake_script() --[[ Main.Main ]]
	local script = Instance.new('LocalScript', Main)

	
	_G.ScriptStillNotTakenDown = true
	script.Parent.Parent.Main.Size = UDim2.new(0,0,0,0)
	script.Parent.Parent.Main.Visible=false
	local par = String
	
	par.Text = ""
	if not game:IsLoaded() then game:IsLoaded() wait() end
	task.wait(1)
		Pattern.Visible=false
	TextLabel.Visible=false
	Main.Visible =true
	Main:TweenSize(UDim2.new(0, 267,0, 174), "InOut", "Quint", 1)
	
	task.wait(1)
	par.Visible=true
	
	Pattern.Visible=true
	TextLabel.Visible=true
	par.Text = "Enter Key To Proceed"
	keylol.Visible = true
	task.wait(0.5)
	par.Text = "just kidding"
	keylol.Visible = false
	task.wait(0.7)
	par.Text = ""
	repeat wait() until game.Players.LocalPlayer
	if _G.ScriptStillNotTakenDown == true then
		par.Text = "Ready to load"
	end
	
	script.Parent:FindFirstChild("LoadScript").Visible = true
	
	script.Parent:FindFirstChild("LoadScript").MouseButton1Click:Connect(function()
		par.Visible=false
		LoadScript.Visible=false
		Pattern.Visible=false
		TextLabel.Visible=false
		Main:TweenSize(UDim2.new(0, 0, 0, 0), "Out", "Bounce", 1)
		task.wait(1)
		pcall(function()
			Intro:Destroy()
			end)
        loadstring(game:HttpGet('https://raw.githubusercontent.com/StupidProAArsenal/UniqueHub/main/Unique_Hub_v2.lua', true))() -- will always update no need to do anything
	end)
	
	
end
coroutine.wrap(UNCWKBO_fake_script)()
local function JZUAXU_fake_script() --[[ Main.Dragify ]]
	local script = Instance.new('LocalScript', Main)

	local UIS = game:GetService("UserInputService")
	local dragSpeed = -math.huge
	
	local dragToggle = nil
	local dragInput = nil
	local dragStart = nil
	local dragPos = nil
	
	function dragify(Frame)
		function updateInput(input)
	        local Delta = input.Position - dragStart
	        local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
	        script.Parent.Position = Position
		end
		
	    Frame.InputBegan:Connect(function(input)
	        if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
	            dragToggle = true
	            dragStart = input.Position
	            startPos = Frame.Position
	            input.Changed:Connect(function()
	                if input.UserInputState == Enum.UserInputState.End then
	                    dragToggle = false
	                end
	            end)
	        end
		end)
		
	    Frame.InputChanged:Connect(function(input)
	        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
	            dragInput = input
	        end
		end)
		
	    game:GetService("UserInputService").InputChanged:Connect(function(input)
	        if input == dragInput and dragToggle then
	            updateInput(input)
	        end
	    end)
	end
	
	dragify(script.Parent)
end
coroutine.wrap(JZUAXU_fake_script)()
