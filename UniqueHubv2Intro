local function ILOZS_fake_script() -- load.OnHover 
	local script = Instance.new('LocalScript', load)

	
	script.Parent.MouseEnter:Connect(function()
	script.Parent.Text = "Click to Load"
	end)
	
	script.Parent.MouseLeave:Connect(function()
	script.Parent.Text = "Load"
	end)
	
end
coroutine.wrap(ILOZS_fake_script)()
local function AEQQCWR_fake_script() -- load.loadscript 
	local script = Instance.new('LocalScript', load)

	local Raw = "https://raw.githubusercontent.com/StupidProAArsenal/UniqueHub/main/Unique_Hub_v2"
	local v = script.Parent.Parent.Parent.Parent.Main
	local z = script.Parent.Parent.Parent.Parent.Parent.uniquehubv2Intro
	local f = false
	function HubLoadStr(istruemode)
		if istruemode == true then
			loadstring(game:HttpGet(Raw, true))()
		else
			if istruemode == false then
				loadstring(game:HttpGet(Raw))()
			end
		end
	
	end
	
	script.Parent.MouseButton1Click:Connect(function()
		
		for i,things in pairs(v:GetChildren()) do
	
			if things:IsA("Frame") then
				things.Visible = f
			end
			if things:IsA("TextLabel") then
				things.Visible = f
			end
			if things:IsA("ScrollingFrame") then
				things.Visible = f
			end
	
	
		end
		v:TweenSize(UDim2.new(0,0,0,0), "InOut", "Quint", 1)
		pcall(function()
			HubLoadStr(true)
		end)
		wait(1)
		z:Destroy()
	end)
end
coroutine.wrap(AEQQCWR_fake_script)()
